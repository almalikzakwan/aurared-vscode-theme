{
	"name": "Aurared",
	"colors": {
		// Editor Base Colors
		"editor.background": "#000b03",
		"editor.foreground": "#E6EDF3",
		"editor.lineHighlightBackground": "#161B22",
		"editor.selectionBackground": "#264F78",
		"editor.inactiveSelectionBackground": "#1F406880",
		"editorCursor.foreground": "#58A6FF",
		"editorWhitespace.foreground": "#3B3F4B",

		// Sidebar Colors
		"sideBar.background": "#060004",
		"sideBar.foreground": "#C9D1D9",
		"sideBarSectionHeader.background": "#181717",
		"sideBarTitle.foreground": "#E6EDF3",
		"sideBar.border": "#B62324",
		
		"editorGroup.border": "#cc00ff",

		// Status Bar Colors
		"statusBar.background": "#B62324",
		"statusBar.foreground": "#ffffff",
		"statusBar.noFolderBackground": "#473b3b",
		"statusBar.debuggingBackground": "#4e1d8b",

		// Panel & Terminal Colors
		"panel.background": "#000000",
		"panel.border": "#B62324",
		"terminal.background": "#000000",
		"terminal.foreground": "#E6EDF3",
		"terminalCursor.foreground": "#58A6FF",

		// Widget Colors
		"editorHoverWidget.background": "#00110b",
		"editorHoverWidget.border": "#0077ff",
		"editorSuggestWidget.background": "#161B22",
		"editorSuggestWidget.foreground": "#C9D1D9",
		"editorSuggestWidget.selectedBackground": "#264F78",
		"peekViewEditor.background": "#0E121A",
		"peekView.border": "#0077ff",

		// Scrollbar Colors
		"scrollbarSlider.background": "#ff00004d",
		"scrollbarSlider.hoverBackground": "#ff00f287",
		"scrollbarSlider.activeBackground": "#6E768199",

		// Notification colors
		"notificationCenterHeader.background": "#0E121A",
		"notifications.background": "#161B22",
		"notifications.foreground": "#E6EDF3",

		// Menu colors
		"menu.background": "#0D1117",
		"menu.foreground": "#E6EDF3",
		"menu.selectionBackground": "#264F78",
		"menu.selectionForeground": "#FFFFFF",

		//title colors
		"titleBar.activeBackground": "#B62324",
        "titleBar.activeForeground": "#FFFFFF",
        "titleBar.inactiveBackground": "#2D2D2D",
        "titleBar.inactiveForeground": "#CCCCCC"

	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
